{
    "latex-workshop.latex.recipes": [
      {
        "name": "xelatex",
        "tools": [
          "xelatex"
        ]
      },
      {
        "name": "xelatex -> bibtex -> xelatex*2",
        "tools": [
          "xelatex",
          "bibtex",
          "xelatex",
          "xelatex"
        ]
      },
      {
        "name": "xelatex -> makeindex-> xelatex*2",
        "tools": [
          "xelatex",
          "makenomencl",
          "makeindex",
          "xelatex",
          "xelatex"
        ]
      },
    ],

    "latex-workshop.latex.tools": [
      {
        "name": "latexmk",
        "command": "latexmk",
        "args": [
          "-synctex=1",
          "-shell-escape",
          "-interaction=nonstopmode",
          "-file-line-error",
          "-pdf",
          "%DOCFILE%"
        ]
      },
      {
        "name": "xelatex",
        "command": "xelatex",
        "args": [
          "-synctex=1",
          "-shell-escape",
          "-interaction=nonstopmode",
          "-file-line-error",
          "%DOCFILE%"
        ]
      },
      {
        "name": "pdflatex",
        "command": "pdflatex",
        "args": [
          "-synctex=1",
          "-shell-escape",
          "-interaction=nonstopmode",
          "-file-line-error",
          "%DOCFILE%"
        ]
      },
      {
        "name": "bibtex",
        "command": "bibtex",
        "args": [
          "%DOCFILE%.bib"
        ]
      },
        {
        "name": "makeindex",
        "command": "makeindex",
        "args": [
          "%DOCFILE%.idx"
        ]
      },
        {
        "name": "makenomencl",
        "command": "makeindex",
        "args": [
            "%DOCFILE%.nlo",
            "-s",
            "nomencl.ist",
            "-o",
            "%DOCFILE%.nls",
            "-t",
            "%DOCFILE%.nlg"
        ]
      },
    ],
    
